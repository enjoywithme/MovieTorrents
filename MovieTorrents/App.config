    <add key="DataPath" value="V:\resource\MT_DATA\" />
    <add key="BtBtHomeUrl" value="https://btbtt18.com/" />
    <add key="DownLoadRootPath" value="V:\resource\temp\" />
    <add key="WebProxy" value="http://10.10.10.202:8080" />
    <add key="AutoDownloadInterval" value="20" />
    <!--自动搜索时间间隔（分钟）-->
    <add key="AutoDownloadSearchPages" value="8" />
    <!--自动搜索的页数-->
    <add key="AutoDownloadSearchHours" value="24" />
    <!--自动搜索的小时-->
  </appSettings>
  <runtime>
